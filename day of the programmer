def dayOfProgrammer(year):
    y = year
    if y<=1917:
        if y%4==0:
            return '12.09.%s' %y
        else:
            return '13.09.%s' %y
    elif y>=1919:
        if y%400 == 0:
            return'12.09.%s' %y
        elif y%4 == 0 and  y%100 !=0:
            return '12.09.%s' %y
        else:
            return'13.09.%s' %y
    elif y ==1918:
        return "26.09.1918"
